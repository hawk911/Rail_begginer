<%= form_for([:admin,@route]) do |f| %>
<%=render 'common/errors' , resourse: @route %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :railway_station_ids , "Станции" %>
    <%= f.collection_select :railway_station_ids, RailwayStation.all, :id, :title, {},{multiple: true} %>
  </div>


  <div class="actions">
    <%= render 'common/submit', f: f, resourse: @route %>
  </div>
<% end %>
