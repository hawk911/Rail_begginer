<tr>
  <td>
     <div id="route_name_<%= route.id %>">
        <%= route.name %>
     </div>
    <%= render 'form', route: route, hidden: true %>
  </td>
  <td><%= link_to t('common.show'), [:admin,route] %></td>
  <td><%= link_to 'Редактировать', '', class: 'edit_route', data: { route_id: route.id } %></td>
  <td><%= link_to t('common.destroy'), [:admin,route], method: :delete, data: { confirm: t('common.destroy_q') } %></td>
</tr>
