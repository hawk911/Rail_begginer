<tr>
  <td>
    <div id="railway_station_title_<%= railway_station.id %>">
      <%= railway_station.title %>
    </div>
    <%= render 'form', station: railway_station, hidden: true %>
  </td>
  <td><%= link_to t('common.show'), [:admin,railway_station] %></td>
  <td><%= link_to 'Редактировать', '', class: 'edit_station', data: { station_id: railway_station.id } %></td>
  <td><%= link_to t('common.destroy'), [:admin,railway_station], method: :delete, data: { confirm:  t('common.destroy_q')  } %></td>
</tr>
